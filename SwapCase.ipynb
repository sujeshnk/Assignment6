      "source": [
        "def swap_case(s):\n",
        "  x=\"\"\n",
        "  for c in s:\n",
        "    if c.isupper():\n",
        "      c=c.lower()\n",
        "    else:\n",
        "        c=c.upper()\n",
        "        x+\"\".join(c)\n",
        "        return x\n",
        "        #swapcase() - Single line\n",
        "        #return s.swapcase()"
      ],
